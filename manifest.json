{
  "manifest_version": 2,
  "name": "LDS Random Song",
  "description": "This extension opens sheet music for random song and plays music.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "http://broadcast3.lds.org/crowdsource/Mobile/LDSMusic/Staging/Collections/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["index.js"]
  }
}
